{
  "name": "khaos-html",
  "version": "0.1.0",
  "description": "A Khaos template for html templates.",
  "main": "index.js",
  "scripts": {
    "start": "gulp watch"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "browserify-shim": {
    "this": "global"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "ignore": "src/lib/**"
        }
      ],
      "browserify-shim",
      "debowerify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cameronjroe/khaos-html.git"
  },
  "keywords": [
    "html",
    "khaos"
  ],
  "author": "Cameron J Roe <cameronjroe@gmail.com> (http://cameronjroe.com/)",
  "license": "MIT",
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "browser-sync": "^2.11.1",
    "browserify": "^13.0.0",
    "browserify-shim": "^3.8.12",
    "debowerify": "^1.3.1",
    "gulp": "^3.9.0",
    "gulp-concat": "^2.6.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.1.1",
    "vinyl-source-stream": "^1.1.0"
  }
}
